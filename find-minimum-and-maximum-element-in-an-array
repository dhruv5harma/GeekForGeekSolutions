class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        long min=Long.MAX_VALUE;
        long max=Long.MIN_VALUE;
        for(int i=0;i<n;i++){
            if(min>a[i]){
                min=a[i];
            }if(max<a[i]){
                max=a[i];
            }
        }
        pair pair = new pair(min,max);
        return pair;
    }
}
